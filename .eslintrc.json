{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style":"off",
        "indent": ["error", "tab"],
        "quotes": ["error", "double"],
        "camelcase":"off",
		"no-tabs":"off",
		"eqeqeq":"error",
		"object-curly-spacing": "off"
    }
}
